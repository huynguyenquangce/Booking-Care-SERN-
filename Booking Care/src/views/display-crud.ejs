<!DOCTYPE html>
<html lang="en">

<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Display CRUD</title>
</head>

<body>
    <table width="100%" style="border: 1px solid black;">
        <tr>
            <th>Email</th>
            <th>FirstName</th>
            <th>LastName</th>
            <th>Address</th>
            <th>Actions</th>
        </tr>
        <% for(var i=0; i < dataTable.length; i++) { %>
            <tr style="text-align: center">
                <td>
                    <%= dataTable[i].email %>
                </td>
                <td>
                    <%= dataTable[i].firstName %>
                </td>
                <td>
                    <%= dataTable[i].lastName %>
                </td>
                <td>
                    <%= dataTable[i].address %>
                </td>
                <td>
                    <a href="/edit-crud?id=<%=dataTable[i].id %>"><button>Edit</button></a>
                    <a href="/delete-crud?id=<%=dataTable[i].id %>"><button>Delete</button></a>
                </td>
            </tr>
            <% } %>
    </table>
</body>

</html>